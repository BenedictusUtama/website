{
  "homepage": "https://riceucsclub.github.io/",
  "name": "cswebsite",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^9.8.5",
    "postcss-cli": "^7.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-alice-carousel": "^1.19.3",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "tailwindcss": "^1.9.6",
    "vanilla-swipe": "^2.2.0",
    "yarn": "^1.22.10"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build:css": "postcss src/index.css -o src/tailwind.css",
    "watch:css": "postcss src/index.css -o src/tailwind.css -w",
    "start": "yarn build:css && react-scripts start",
    "build": "yarn build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.1.0"
  }
}
